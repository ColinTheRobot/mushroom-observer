<h2><%= @vernacular.to_s %></h2>
<div>
	<ul>
		<li>uri: <%= @vernacular.uri %></li>
		<li>label: <%= @vernacular.label %></li>
		<li>
			features: 
				<ul> 
					<% @vernacular.features.each do |feature| %>
					<li>
						<%= feature["feature"] + ": " + feature["value"] %>
					</li>
					<% end %>
				</ul>
		</li>
	</ul>
</div>