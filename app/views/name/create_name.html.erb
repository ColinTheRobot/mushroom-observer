<%
  @title ||= :create_name_title.t

  new_tab_set do
    add_tab(:all_objects.t(:type => :name), :action => 'list_names')
  end
%>

<div class="row">
  <div class="col-md-6">
<%= form_for(:name,
      :url => add_query_param(:action => :create_name)) do |form| %>
  <%= render(:partial => 'form_name', :locals => {form: form}) %>
  <center><%= submit_tag(:CREATE.l, class:"btn btn-default btn-lg") %></center>
<% end %>
  </div>
</div>
